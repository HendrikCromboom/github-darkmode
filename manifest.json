{
  "name": "Github Darkmode",
  "description": "Darkmode for Github",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
    "16": "./github16.png",
    "32": "./github32.png",
    "64": "./github64.png",
    "128": "./github128.png"
  },
  "background": {
    "scripts": ["./background.js"]
  },
  "options_page": "./options.html",
  "browser_action": {
    "default_popup": "./popup.html"
  },
  "permissions": [
    "tabs",
    "https://github.com/*"
    ]
}